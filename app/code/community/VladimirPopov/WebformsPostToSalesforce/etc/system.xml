<?xml version="1.0"?>
<config>
    <sections>
        <webforms>
            <groups>
                <proccessresult translate="label comment">
                    <comment></comment>
                    <label>Post data form to Salesforce</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1000</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enable translate="label comment">
                            <comment>
                                <![CDATA[data of form Product Enquiry will be post to Salesforce]]>
                            </comment>
                            <label>Enable post data form to Salesforce</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enable>
                        <oid translate="label">
                            <label>Organization ID</label>
                            <frontend_type>text</frontend_type>
                            <comment>Enter your Salesforce Organization ID</comment>
                            <sort_order>11</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </oid>
                    </fields>
                </proccessresult>
            </groups>
        </webforms>
    </sections>
</config>